      "blocks": {
        "d89926c8-c720-4303-bbe7-860e22e31faa": {
          "type": "heading",
          "settings": { "heading": "Slim, Sleek and Stylish", "heading_size": "h1" }
        },
        "f83a7a75-5e17-4f92-bad7-9341e24a4dd0": {
          "type": "text",
          "settings": {
