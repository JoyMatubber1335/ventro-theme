    "rich_text_tfjVC4": {
      "type": "rich-text",
      "blocks": {
        "heading_FBkMeg": { "type": "heading", "settings": { "heading": "OUR ENGRAVINGS", "heading_size": "h1" } },
        "text_WPthXA": {
          "type": "text",
          "settings": {
            "text": "<p>At <strong>Ventro</strong>, we bring your vision to life with our expert engraving services. Whether you’re adding your initials, a meaningful quote, or your company logo, our precision laser engraving technology guarantees a flawless finish. Explore our gallery of previous custom engravings for inspiration and see how we transform our wallets into personal works of art.</p>"
          }
        }
      },
      "block_order": ["heading_FBkMeg", "text_WPthXA"],
      "settings": {
        "desktop_content_position": "center",
        "content_alignment": "center",
        "color_scheme": "",
        "full_width": true,
        "padding_top": 40,
        "padding_bottom": 52
      }
    },
    "collapsible_content_6bJg6T": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible_row_GYBwfT": {
          "type": "collapsible_row",
          "settings": {
            "heading": "What are the charges for engraving?",
            "icon": "price_tag",
            "row_content": "<p>Engraving costs:</p><ul><li><strong>Single Position</strong>: 250 Taka</li><li><strong>Additional Positions</strong>: 200 Taka per position.</li></ul>",
            "page": ""
          }
        },
        "collapsible_row_6LK7VV": {
          "type": "collapsible_row",
          "settings": {
            "heading": "What file format is required for custom designs?",
            "icon": "ruler",
            "row_content": "<p><strong>For Texts:</strong></p><p>Mention the text in the box above the 'Buy Now' button along with the position. </p><p><em>For example: Position 1- Mr.President</em></p><p>You may include the font name if you have any in mind as well. </p><p></p><p><strong>For Silhouette style pictures:</strong></p><p>We require your design in a <em>transparent PNG format</em> to ensure the best engraving quality. You can send the file by replying to our confirmation email or contacting us directly.</p>",
            "page": ""
          }
        },
        "collapsible_row_NMYAmN": {
          "type": "collapsible_row",
          "settings": {
            "heading": "How long does it take to complete an engraving order?",
            "icon": "stopwatch",
            "row_content": "<p><strong>For in-store purchases</strong>, the engraving process takes 10 to 15 minutes. <br/><br/><strong>For online purchases</strong>, once payment and the design are confirmed, engraving typically takes 48-72 hours. </p><p>You’ll be notified as soon as your product is ready for pickup or delivery.</p>",
            "page": ""
          }
        },
        "collapsible_row_QWCwHe": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Why is full advance payment required for engraving orders?",
            "icon": "question_mark",
            "row_content": "<p>We require <strong>full payment upfront</strong> because once the product is engraved, it cannot be resold or returned due to personalization.</p>",
            "page": ""
          }
        },
        "collapsible_row_E6jmqp": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Do you offer refunds or returns for engraved products?",
            "icon": "return",
            "row_content": "<p>No, we do not offer refunds, returns, or exchanges on engraved products, as they are personalized specifically for you.</p><p> Please double-check your design and details before confirming the order.</p>",
            "page": ""
          }
        }
      },
